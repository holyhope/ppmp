        <class>fr.onlineCV.entities.ObtainPK</class>
        <class>fr.onlineCV.entities.Phone</class>
        <class>fr.onlineCV.entities.Skill</class>
        <class>fr.onlineCV.entities.Study</class>
        <class>fr.onlineCV.entities.StudyPK</class>
        <class>fr.onlineCV.entities.Hobby</class>
